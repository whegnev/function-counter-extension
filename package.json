{
  "name": "function-counter-extension",
  "displayName": "Function Counter Extension",
  "description": "Counts and displays the number of functions, if statements, for and while loops in your code. Shows real-time statistics in the status bar as you type.",
  "version": "1.0.0",
  "engines": {
    "vscode": "^1.74.0"
  },
  "publisher": "whegnev",
  "categories": [
    "Other"
  ],
  "keywords": [
    "stats",
    "analyzer",
    "code metrics",
    "functions",
    "loops",
    "if statements"
  ],
  "activationEvents": [
    "onStartupFinished"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "function-counter-extension.showStats",
        "title": "Show Code Statistics",
        "category": "Code Stats"
      },
      {
        "command": "function-counter-extensionr.refreshStats",
        "title": "Refresh Code Statistics", 
        "category": "Code Stats"
      }
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/whegnev/function-counter-extension"
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/vscode": "^1.74.0",
    "@types/mocha": "^10.0.6",
    "@types/node": "18.x",
    "@typescript-eslint/eslint-plugin": "^6.13.1",
    "@typescript-eslint/parser": "^6.13.1",
    "eslint": "^8.54.0",
    "typescript": "^5.3.2",
    "@vscode/test-cli": "^0.0.4",
    "@vscode/test-electron": "^2.3.6"
  }
}